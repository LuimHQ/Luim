{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }], 
		"paths": {
			"@components/*": ["./src/components/*"],
			// "@contexts/*": ["./src/contexts/*"],
			// "@pages/*": ["./pages/*"],
			"@styles/*": ["./src/styles/*"],
			// "@utils/*": ["./utils/*"],
			// "@hooks/*": ["./hooks/*"],
			// "@types/*": ["./types/*"],
			// "@public/*": ["./public/*"],
			"@lib/*": ["./src/lib/*"]
		}
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
